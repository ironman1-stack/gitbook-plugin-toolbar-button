    ],
    "gitbook": {
        "properties": {
            "icon": {
                "type": "string",
                "title": "font awesome icon. ex:  'fa-globe'",
                "required": true
            },
            "url": {
                "type": "string",
                "title": "Url for the button",
                "required": true
            },
            "label": {
                "type": [ "string", "object" ],
                "title": "Label for the edit button",
                "default": ""
            }
        }
    }
